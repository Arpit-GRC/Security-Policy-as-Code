{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyAdminAccessToNonPrivilegedRoles",
      "Effect": "Deny",
      "Action": "*",
      "Resource": "*",
      "Condition": {
        "StringNotEquals": {
          "aws:PrincipalArn": [
            "arn:aws:iam::<ACCOUNT_ID>:role/Admin-Role",
            "arn:aws:iam::<ACCOUNT_ID>:role/BreakGlass-Role"
          ]
        }
      }
    }
  ]
}

